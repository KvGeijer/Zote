fn fib(n) {
	if n < 0 {
		return 0;
	};
	
	if n == 0
		1
	else if n == 1 
		1
	else {
		var x = 2;
		var y;

		x = fib(n-2) + (y = fib(n-1));
	};
};


var i = -1;
while (i = i + 1) < 26 {
	var start = time();
	var res = fib(i);
	var dur = time() - start;

	print("Fib(" + str(i) + ") = " + str(res));
	print("Time: " + str(dur));
	print("---------------");
};
